<demo-block title="基础用法">
  <demo-code bindshowMore="showMore" activeId="{{activeId}}" name="pageHook" type="basic" num="0">
    <view>
      onLoad: 生命周期函数--监听页面加载
    </view>
  </demo-code>
  <demo-code bindshowMore="showMore" activeId="{{activeId}}" name="pageHook" type="basic" num="1">
    <view>
      onShow: 生命周期函数--监听页面显示
    </view>
  </demo-code>
  <demo-code bindshowMore="showMore" activeId="{{activeId}}" name="pageHook" type="basic" num="2">
